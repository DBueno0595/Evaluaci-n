@model IEnumerable<BdUsuarios.Models.Users>

    <table class="table table-hover">
        <tr>
            <th>
                ID USER
            </th>
            <th>
                USER
            </th>
            <th>
                EMAIL
            </th>
            <th>
                PASSWORD
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.IdUser)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Usuario)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Contraseña)
                </td>
                <td>
                    @Html.ActionLink("Editar", "Edit", new { id = item.IdUser })|
                    @Html.ActionLink("Eliminar", "Delete", new { id = item.IdUser })|
                </td>
            </tr>
        }

        <a href="" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">Primary link</a>

    </table>
